<script type="text/javascript">
    function collaspse_submenu(el) {

        if (!el.hasClass('collapsed') && !el.hasClass('always_open')) {
            el.addClass('collapsed');
        }

    }

    function spread_submenu(el) {
        if (el.hasClass('collapsed')) {
            el.removeClass('collapsed');
        }
    }


    $('#menu-id li.sub').each(function(i,el) {
        collaspse_submenu($(el));
    });

    $('#menu-id li.sub').each(function(i,el) {
        var elem = $(el);
        if (elem.hasClass('selected'))
            spread_submenu(elem);


        elem.children('a').click(function() {
            el = $(this).parent();
            if (!el.hasClass('collapsed')) {
                collaspse_submenu(elem)
            }else{
                spread_submenu(elem)
            }
            return false;
        })

    });


</script>
