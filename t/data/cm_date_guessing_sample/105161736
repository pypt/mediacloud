	function clipSlash(str){
   if(str.charAt(str.length-1) == "/"){ str = str.substr(0, str.length - 1);}
   return str
	 }
	 
	 var scriptSrc;
	 		 if (scriptSrc != null) {
			 		var AdZone = scriptSrc.split(";");
					AdZone = AdZone[0].split("/");
					var y = AdZone.splice(0,4);
					var zone = "";

					for (i=0, len=AdZone.length; i< len; i++) {
							zone+= AdZone[i];
									if (AdZone[i] != "") {zone+= "/";}
								}
					zone = clipSlash(zone);
			} else {
					zone = "trb.latimes/";
	}

	var addtl = "latimes&caid=/&sec=root&ss=&ct=sectionfront&tag=&akv1="+zone;
