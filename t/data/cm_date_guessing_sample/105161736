<script type="text/javascript">

// LAT SSOR Registration Nav changes for LATFE-217. 
jQuery('#memberLoginInfo').css('visibility', 'hidden');
 
 jQuery(document).ready(function($) {
    if (document.cookie.indexOf("c_mId") < 0) {
        $('#registrationNavigation #registrationNavigationHeader span, a#flexibleSignUpLink, a#flexibleSignInLink').remove();
        $('#ssorNavHeader, #registrationNavigationBody, #ssorMiniModal,')
       .removeAttr('style');
        $('#registrationNavigationHeader').append('<a href="#" data-reg-handler="signUpHandler" id="flexibleSignUpLink" class="subscribeButton"></a> <a href="#" data-reg-handler="signInHandler" id="flexibleSignInLink" class="logInText">Log In</a>');
    } else {
        $('#registrationNavigationHeader').prepend('Hi, ');
        $('#ssorMiniModal').remove();
        $('#registrationNavigationBody').append('<ul id="ssorMiniModal"><li id="profileLink"><span><a href="http://membership.latimes.com/profile">Profile</a></span></li><li id="newsletterLink"><span><a href="http://membership.latimes.com/alerts-newsletters">Newsletters</a></span></li><li><span data-reg-handler="signOutHandler" id="signOutLink"><a href="javascript:void(0)">Sign Out</a></span></li></ul>');
    }
    
    $('#memberLoginInfo').css('visibility', 'visible');
    
    registration.manager.events.getEventManagerInstance().addListener ('reg_after_login', function() {
        setTimeout(function(){
        $('#registrationNavigationHeader').prepend('Hi, ');
        $('#ssorMiniModal').remove();
        $('#registrationNavigationBody').append('<ul id="ssorMiniModal"><li id="profileLink"><span><a href="http://membership.latimes.com/profile">Profile</a></span></li><li id="newsletterLink"><span><a href="http://membership.latimes.com/alerts-newsletters">Newsletters</a></span></li><li><span data-reg-handler="signOutHandler" id="signOutLink"><a href="javascript:void(0)">Sign Out</a></span></li></ul>');
        },500);
    });
    
});

</script>
 
