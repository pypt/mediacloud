<script type="text/javascript">	

function mouseoverActiontooltip(event)
{
jQuery(".toolTip").before("<p id='toolTip'>"+ this.rel + "</p>");
jQuery("#toolTip").css("z-index", "2137483608");
jQuery("#toolTip").css("_margin-left", "-60px");
jQuery("#toolTip").css("margin-left", "-10px");
jQuery("#toolTip").css("margin-top", "21px");
}

function mouseoutActiontooltip(event)
{
jQuery("#toolTip").remove();
}

jQuery('.toolTip').bind('mouseover', mouseoverActiontooltip);
jQuery('.toolTip').bind('mouseout', mouseoutActiontooltip);

</script>
<script type="text/javascript" src="http://latimesblogs.latimes.com/common/sidebar-js-2.js"></script>
<script type="text/javascript">
function vidfix(){
	var myobject = jQuery('object[id^="myExperience"][class!="BrightcoveExperience"]');
	for(i=0; i<myobject.length; i++) {
	  var placeholder = document.createElement("div");
	  placeholder.appendChild(myobject[i].cloneNode(true));
	  var objs = placeholder.getElementsByTagName("object");
	  obj = objs[0];
	  obj.removeAttribute("type");
	  obj.removeAttribute("data");
	  obj.removeAttribute("width");
	  obj.removeAttribute("height");
	  var widthItem = document.createElement('param');
    	widthItem.setAttribute('name','width');
    	widthItem.setAttribute('value','620');

	  var heightItem = document.createElement('param');
    	heightItem.setAttribute('name','height');
    	heightItem.setAttribute('value','349');

    var itemType = document.createElement('param');
      itemType.setAttribute('name', 'contentItemType');
      itemType.setAttribute('value', 'embed')

    var itemPath = document.createElement('param');
    var windowPath = window.location.pathname;
    var regExPath = /\/\w+\//;
    var blogPath = windowPath.match(regExPath);
      itemPath.setAttribute('name', 'sectionName');
      itemPath.setAttribute('value', blogPath);
      
	  jQuery(obj).append(widthItem).append(heightItem).append(itemType).append(itemPath).addClass('BrightcoveExperience');
	  myobject[i].parentNode.replaceChild(placeholder.firstChild, myobject[i]);
	  placeholder = null;
	}

	brightcove.createExperiences();
	
}

vidfix();
</script>

