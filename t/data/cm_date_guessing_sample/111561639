function printProperties(obj) {
    var names='Type: ' + obj + '\n Properties:\n';
    for (var i in obj) {
        names += "\t" + i + ";";
    }
    window.alert(names);
}

function turnOnTab(tabName) {
        
	// first, display the selected content, and change the content area's color
	document.getElementById(tabName).style.display = 'block'

	// then, updated the style of the selected tab
	selectedTabItem=document.getElementById('nav_' + tabName);
	selectedTabItem.className ='TLBoxTabSelected';

}

function turnOffTab(tabName) {
	// first, hide the selected content
	document.getElementById(tabName).style.display = 'none';

	// then, update the style of the UNselected tab
	unSelectedTabItem=document.getElementById('nav_'+ tabName);
	unSelectedTabItem.className = 'TLBoxTab';

}

function initializeTabs () {
	selectTab (randomTab);
}

function selectTab (tabName) {
	for (i=0; i<tabQty; i++) {
		turnOffTab(tabs[i])
	}
	turnOnTab(tabName);	
}

