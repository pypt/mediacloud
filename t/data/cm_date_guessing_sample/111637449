<script src="http://www.10news.com/sps-kgtv-theme/js/main.js?browserId=other&amp;minifierType=js&amp;languageId=en_US&amp;b=6110&amp;t=1372723714000" type="text/javascript"></script>



	

	

	<script type="text/javascript">
		// <![CDATA[
			

			
		// ]]>
	</script>






 
		<script type="text/javascript">
			/*
 * Scripps Most Popular Pages Widget v0.2
 * http://devstudio.scrippscripts.com
 *
 * Copyright 2011, Scripps Media, Inc
 *
 * May 2011 - Initial Release (Joe Lee)
 * Sep 2012 - Updated for New Release of EndPlay and McGraw Hill Migration
 */

(function($)
{

    $.fn.scrippsMostPopularPages = function( mppOptions )
    {

        var mppDefaultSettings =
        {

            width               :   '313px',
            height              :   '200px',
            onDevSite           :   false,
            header              :   'Top Stories Trending Now',
            numberOfHeadlines   :   5,
            hostUrl             :   'newsnet5.com',
            siteUrl             :   'www.newsnet5.com',
            imageUrl            :   'http://media2.newsnet5.com/html/images/',
            apiKey              :   '06de469f576be61d2dcc350e4e602f0a'

        }

        var mppOptions = $.extend(mppDefaultSettings, mppOptions);

		_el = this;
        _onDevSite = false;

        _url = 'http://api.chartbeat.com/toppages/?host=' + mppOptions.hostUrl + '&limit=15&apikey=' + mppOptions.apiKey;

        _currentPageTitle = '';
        _currentLinkPath = '';
        _currentPageVisitors = '';

        _oldRank = 0;
        _currentRank = 0;

        _currentPageRankList = new Array();
        _currentPageTitleList = new Array();
        _currentLinkPathList = new Array();
        _currentPageVisitorsList = new Array();
        _currentTrendingList = new Array();

        _oldPageRankList = new Array();
        _oldPageTitleList = new Array();
        _oldLinkPathList = new Array();
        _oldPageVisitorsList = new Array();
        _oldTrendingList = new Array();

        _isFirstTime = true;
        _isSecondTime = true;

        setInterval
        (
            function()
            {

                getChartbeatData();

            }, 15000
        );

        getChartbeatData();

        function getChartbeatData()
        {
            $.ajax(
            {

                url:            _url,
                dataType:       'jsonp',
                jsonp:          'jsonp',
                success:        function(data)
                                {
                                    chartbeatCallback(data);
                                }
            });

        }

        function chartbeatCallback(jsonData)
        {
        _currentNumberOfElements = 0;
            _oldRank = 0;
            _currentRank = 0;

            cloneLists();
            clearLists();

            $.each(jsonData, function(ndx, item)
            {

                _currentPageTitle = item.i;
                _currentLinkPath = item.path;
                _currentPageVisitors = item.visitors;


				// Make sure the path starts with the host (turnto23.com, and 10news.com are not returnig)
				if (_currentLinkPath.indexOf(mppOptions.hostUrl) != 0) {
					_currentLinkPath = mppOptions.hostUrl + _currentLinkPath;
				}
				
				// Strip the call letters and section information out of the title, we just want headlines
				var firstDashInTitle = _currentPageTitle.indexOf("-");
				if (firstDashInTitle > -1) {
					_currentPageTitle = _currentPageTitle.substr(firstDashInTitle + 1);
					
					var lastDashInTitle = _currentPageTitle.indexOf(" - ");
					if (lastDashInTitle > -1)
						_currentPageTitle = _currentPageTitle.substr(0, lastDashInTitle);
				}
				
				
               if (_currentLinkPath.indexOf('-') == -1 || _currentPageTitle == "" ||  _currentLinkPath.indexOf("-staging") > -1 ||
					_currentNumberOfElements >= mppOptions.numberOfHeadlines || _currentPageTitle == "ERROR" || _currentLinkPath.indexOf("serp") > -1)
                {
                    // Ignore these elements.
                }
                else
                {
                    if ($.inArray(_currentPageTitle, _currentPageTitleList) < 0)
                    {
                        _currentNumberOfElements++;

                        _currentPageRankList.push(_currentNumberOfElements);

                        tempPageTitle = _currentPageTitle;
                        hashPos = tempPageTitle.indexOf('#');
                        if (hashPos > 0)
                        {
                            tempPageTitle = tempPageTitle.substr(0, hashPos);
                        }


						// Strip up to the first


                        _currentPageTitleList.push(tempPageTitle);

                        _currentLinkPathList.push(_currentLinkPath);
                        _currentPageVisitorsList.push(_currentPageVisitors);

                        _oldRank = $.inArray(_currentPageTitle, _oldPageTitleList) + 1;
                        _currentRank = _currentNumberOfElements;

                        if (_oldRank == 0)
                        {
                            _currentTrendingList.push('^');
                        }
                        else if (_currentRank == _oldRank)
                        {
                            _currentTrendingList.push('-');
                        }
                        else if (_currentRank > _oldRank)
                        {
                            _currentTrendingList.push('down');
                        }
                        else if (_currentRank < _oldRank)
                        {
                            _currentTrendingList.push('^');
                        }
                    }
                }

                displayChartbeatWidget();

            });


        }

        function displayChartbeatWidget()
        {
        	 $('#MostPopularPages').html('');
            $('#MostPopularPages')
                .css('width', '313px')
                .css('font-size', '100%')
                .css('background-image', 'url(' + mppOptions.imageUrl + 'TopShadow.gif)')
                .css('background-repeat', 'no-repeat')
                .css('background-position', 'right top')
                .css('background-color', '#d2d2d2');

            $('<div/>')
                .attr('id', 'mostPopularPagesWidgetShadow')
                .css('width', '308px')
                .css('border', '1px solid #d2d2d2')
                .css('background-color', '#fff')
                .css('padding', '0px')
                .css('margin', '0px 5px 0px 0px')
                .appendTo('#MostPopularPages');

            $('<div/>')
                .attr('id', 'mostPopularPagesWidgetTitle')
                .css('width', '303px')
                .css('background-color', '#d2d2d2')
                .css('color', '#000')
                .css('border-top', '5px solid #fff')
                .css('font-family', 'Arial,Helvetica,Sans-Serif')
                .css('font-size', '11pt')
                .css('font-weight', 'bold')
                .css('margin-left', '5px')
                .appendTo('#mostPopularPagesWidgetShadow');

            $('<span/>')
                .css('width', '303px')
                .css('float', 'left')
                .css('background-color', '#fff')
                .html(mppOptions.header)
                .appendTo('#mostPopularPagesWidgetTitle');


            for (idx=0; idx < _currentPageTitleList.length; idx++)
            {

                if (_currentTrendingList[idx] == '-')
                {
                    _animationBackgroundColor = '#154e50';
                    _trendingImage = '<img src="' + mppOptions.imageUrl + 'Steady.gif" alt="Steady" />';
                }
                else if (_currentTrendingList[idx] == '^')
                {
                    _animationBackgroundColor = '#028b0b';
                    _trendingImage = '<img src="' + mppOptions.imageUrl + 'Up.gif" alt="Up" />';
                }
                else if (_currentTrendingList[idx] == 'down')
                {
                    _animationBackgroundColor = '#b10803';
                    _trendingImage = '<img src="' + mppOptions.imageUrl + 'Down.gif" alt="Down" />';
                }

                if (_isSecondTime)
                {
                    _trendingImage = '&nbsp;';
                }

                $('<div/>')
                    .attr('id', 'row' + idx)
                    .css('width', '298px')
                    .css('height', '50px')
                    .css('display', 'table')
                    .css('clear', 'both')
                    .css('padding-top', '4px')
                    .css('margin-left', '5px')
                    .appendTo('#mostPopularPagesWidgetShadow');

                if (idx != 0)
                {

                    $('#row' + idx)
                        .css('border-top', '1px solid #d2d2d2');

                }
                else
                {

                    $('#row' + idx)
                        .css('padding-top', '10px');

                }

                $('<div/>')
                    .attr('id', 'cell' + idx)
                    .css('width', '293px')
                    .css('display', 'table-cell')
                    .css('vertical-align', 'middle')
                    .appendTo('#row' + idx);

                $('<div/>')
                    .css('width', '38px')
                    .css('float', 'left')
                    .css('font-family', 'Arial,Helvetica,Sans-Serif')
                    .css('font-size', '9pt')
                    .css('text-align', 'center')
                    .css('margin-bottom', '5px')
                    .css('display', 'inline-block')
                    .css('padding-top', '0px')
                    .html(_trendingImage)
                    .appendTo('#cell' + idx);

                $('<div/>')
                    .css('width', '250px')
                    .css('float', 'left')
                    .css('font-family', 'Arial,Helvetica,Sans-Serif')
                    .css('font-size', '9pt')
                    .css('margin-bottom', '5px')
                    .css('display', 'inline-block')
                    .css('vertical-align', 'middle')
                    .html('<a style="display:inline-block;vertical-align:middle;" class="active" href="http://www.' + _currentLinkPathList[idx] + '" target="_top">' + _currentPageTitleList[idx] + '</a>')
                    .appendTo('#cell' + idx);

            }

            if (_isFirstTime && _isSecondTime)
            {

                _isFirstTime = false;

            }
            else
            {

                _isSecondTime = false;

            }


            $('<div/>')
                .attr('id', 'BottomShadow')
                .css('width', '313px')
                .css('height', '5px')
                .css('background-color', '#fff')
                .appendTo('#MostPopularPages');

            $('<span/>')
                .css('width', '308px')
                .css('float', 'right')
                .css('height', '5px')
                .css('background-color', '#d2d2d2')
                .html('')
                .appendTo('#BottomShadow')

            if (_onDevSite)
            {

                $('<div/>')
                    .attr('id', 'FooterRow')
                    .css('width', '323px')
                    .css('min-height', '18px')
                    .css('background-color', '#ddd')
                    .css('clear', 'both')
                    .appendTo('#MostPopularPages');

                $('<span/>')
                    .css('width', '48%')
                    .css('float', 'left')
                    .css('font-family', 'Arial,Helvetica,Sans-Serif')
                    .css('font-size', '8pt')
                    .css('text-align', 'left')
                    .css('padding-top', '2px')
                    .css('padding-left', '5px')
                    .css('margin-bottom', '5px')
                    .css('min-height', '15px')
                    .css('background-color', '#ddd')
                    .html('Scripps Widget Engine')
                    .appendTo('#FooterRow');

                $('<span/>')
                    .css('width', '48%')
                    .css('float', 'left')
                    .css('font-family', 'Arial,Helvetica,Sans-Serif')
                    .css('font-size', '8pt')
                    .css('text-align', 'right')
                    .css('padding-top', '2px')
                    .css('padding-right', '5px')
                    .css('margin-bottom', '5px')
                    .css('min-height', '15px')
                    .css('background-color', '#ddd')
                    .html('<a href="http://www.scrippsscripts.com/scripps/widgets/mostPopularPages/demo.html" style="background-color:#ddd;">Get Customized Widget</a>')
                    .appendTo('#FooterRow');

            }
        }

        function cloneLists()
        {

            _oldPageRankList = _currentPageRankList.slice(0);
            _oldPageTitleList = _currentPageTitleList.slice(0);
            _oldLinkPathList = _currentLinkPathList.slice(0);
            _oldPageVisitorsList = _currentPageVisitorsList.slice(0);
            _oldTrendingList = _currentTrendingList.slice(0);

        }

        function clearLists()
        {

            _currentPageRankList = [];
            _currentPageTitleList = [];
            _currentLinkPathList = [];
            _currentPageVisitorsList = [];
            _currentTrendingList = [];

        }

    };

})(jQuery); 
		</script>
	


</div> <!-- end #wrapper -->
</div> <!-- end #mfl -->

