	<tr>
		<td valign="middle">
			<div class="miscAd cube">	
				
				<div id="cubeContainer2">
					<iframe id="CubeAd2" name="CubeAd2" align="center" marginwidth="0" marginheight="0" frameborder="0" scrolling="no" width="300" height="250"></iframe>
