<script type="text/javascript" src="http://www.latimes.com/includes/projects//js/jquery/jquery-1.4.2.min.js"></script>

<script type="text/javascript">
jQuery.noConflict();
/******************************************
Ad Refresh
******************************************/
    ad_swapper = {
        init : function () {
            // Grab the container divs for the iframes      
            this.bannerCont = document.getElementById('leaderContainer');
            this.cubeCont = document.getElementById('cubeContainer');
            this.skyCont = document.getElementById('skyContainer');
            // Now, we have to make a copy of what's in the container divs so we can wipe them on refresh
            // This is necessary because friendly iframes allow the expandable ads to write code in the DOM of the parent document. When we refresh, we have to wipe any extra code outside of the iframes to make way for the new ad
            this.bannerHTML = this.bannerCont.innerHTML;
            this.cubeHTML = this.cubeCont.innerHTML;
            this.skyHTML = this.skyCont.innerHTML;
            return this.load();
        },
        clear : function () {
            this.bannerCont.innerHTML = this.bannerHTML;
            this.cubeCont.innerHTML = this.cubeHTML;
            this.skyCont.innerHTML = this.skyHTML;
        },
        load : function () {
            document.getElementById("BannerAd").src = 'http://latimesblogs.latimes.com/california-politics/leaderboard.html';
            document.getElementById("CubeAd").src = 'http://latimesblogs.latimes.com/california-politics/cubead.html';
            document.getElementById("SkyAd").src = 'http://latimesblogs.latimes.com/california-politics/skyad.html';
        },
        swap : function () {
            this.clear();
            this.load();
        }
    };

    jQuery(document).ready(function(){
        // Finally, we can actually wait until document.ready to set the SRC of the ad frames (there is no SRC by default), making the ads the last thing loaded on the page.
        ad_swapper.init();


        function get_twitter_count(url){

        }

        jQuery('div.tweet_container').each(function(){
            var url = jQuery(this).attr('data-url');
            var that = this;
            jQuery.ajax({
                url: "http://urls.api.twitter.com/1/urls/count.json?url=" + url,
                dataType: 'jsonp',
                success: function(data){
                    jQuery('div.tweet_count', that).html(data.count);
                },
                error: function(){

                }
            });         
        });
    });

function count_increase(element) {
    var eso = jQuery(element);
    var count_div = jQuery("div.tweet_count", eso.parent());
    if(!count_div.data("has_clicked")) {
        var count = count_div.html();
        if(count) {
            count = parseInt(count, 10) + 1;
        }
        else {
            count = 1;
        }
        count_div.html(count);
        count_div.data("has_clicked", true);
    }
}

function font_size(toggle) {
    var post_body_selector = jQuery('.entry-content');
    var current_size = parseInt(post_body_selector.css('font-size').replace('px', ''));
    if (toggle === 'up'){
        post_body_selector.css({'font-size': (current_size + 1) + 'px'})
    }
    else if (toggle === 'down'){
        post_body_selector.css({'font-size': (current_size - 1) + 'px'})
    }
}

</script>


