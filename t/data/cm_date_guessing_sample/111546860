var CT = ""; //ct
var Cat = jQuery.trim( jQuery('div.entry-footer-right a').first().html() );  //ss
var CatTag = '';

if (Post.indexOf("inpage.") > 0) {
			CatTag = Post;
			CT = Post;
		} else if (Post.indexOf("cat.") > 0) {
			CatTag = JTcat;
			CT = "category";
		} else {	
			CT = "post";
			Post = jQuery.trim(omniture_blog_entrytitle.slice(38));
				if (Cat != "" && Cat != undefined) {
					CatTag = Cat;
				} else {
					CatTag = "none";
				}
	} 

var addtl = "latimes&caid="+ Post +"&sec=California Politics&ss="+ CatTag +"&ct="+ CT +"&akv1=trb.latimes/news/local/politics/blogs&embed=true";

if (typeof(jt) !== "undefined") { jt("http://latimes.jump-time.net/latimes", addtl, false); }

