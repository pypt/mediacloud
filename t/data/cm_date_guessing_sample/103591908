          fjs.parentNode.insertBefore(js, fjs);
          }(document, 'script', 'facebook-jssdk'));</script><div data-width="150" data-show-faces="true" data-send="false" data-layout="button_count" data-font="arial" data-action="recommend" class="fb-like"></div></div><div class="b-story_detail__social-item b-story_detail__social-item_twitter"><a href="https://twitter.com/share" data-via="RidusNews" data-lang="en" class="twitter-share-button">Твитнуть</a><script>!function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0];if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src="//platform.twitter.com/widgets.js";fjs.parentNode.insertBefore(js,fjs);}}(document,"script","twitter-wjs");</script></div><div class="b-story_detail__social-item b-story_detail__social-item_google"><script type="text/javascript" src="https://apis.google.com/js/plusone.js">
