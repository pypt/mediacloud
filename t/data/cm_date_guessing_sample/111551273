<img src="http://www.sacbee.com/static/capalert/insider/images/cap_alert_banner.gif" width="620" height="102" border="0" usemap="#Map" alt="Capitol Alert" />
<map name="Map" id="Map">
  <area shape="rect" coords="2,2,617,80" href="http://blogs.sacbee.com/capitolalertlatest/" />
  <area shape="rect" coords="231,81,611,106" href="http://www.sacbee.com/insider" />
