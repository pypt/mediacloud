<script type="text/javascript">
for (i=1; i<7; i++) {
	var q = document.getElementById('followUs').children[i].children[0];
	if(q.addEventListener) {
		q.addEventListener('mouseover', FUshow, false);
		q.addEventListener('mouseout', FUhide, false);
	} else if (q.attachEvent) {
		q.attachEvent('onmouseover', FUshow);
		q.attachEvent('onmouseout', FUhide);
	}
}
function FUshow(event) {
	document.getElementById('FUm').style.display = 'none';
	try {
		q = this.id.substring(0,3);
	} catch(err) {}
	try {
		q = event.srcElement.id.substring(0,3);
	} catch(err) {}
	document.getElementById(q).style.display = 'inline';
}
function FUhide() {
	try {
		q = this.id.substring(0,3);
	} catch(err) {}
	try {
		q = event.srcElement.id.substring(0,3);
	} catch(err) {}
	document.getElementById(q).style.display = 'none';
	document.getElementById('FUm').style.display = 'inline';
}
</script>

