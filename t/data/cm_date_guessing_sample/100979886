<script>
 var vt = document.getElementById("viewedTab");viewedTab
 var ct = document.getElementById("commentTab");
 var vhtml = vt.innerHTML
 var chtml = ct.innerHTML
 vhtml = vhtml.replace(/<UL>/ig, "<OL>");
 chtml = chtml.replace(/<UL>/ig, "<OL>");
 vt.innerHTML=vhtml;
