								jQuery('.mainLoginButton').hide();
								jQuery('.loginForm').slideUp('fast');
								jQuery('.registerForm').slideDown('fast');
							})
							
							jQuery('.cancelButton').click(function(){
								// jQuery(this).closest('form').slideUp('fast');
								// jQuery('.fbLogin b').slideDown('fast');
								// jQuery('.loginButtons').slideDown('fast');
								// jQuery('.loginTitle').slideDown('fast');
								// jQuery('.registerTitle').slideUp('fast');
								// jQuery('.widget_wcfacebooktiewidget .widget_wccontentsubmiter').slideDown('fast');
								jQuery('.loginForm').slideUp('fast');
								jQuery('.registerForm').slideUp('fast');
								jQuery('.mainLoginButton').slideDown();
								
							})

							jQuery('.loginButton').click(function(e){

								jQuery('.loginForm').slideDown('fast');
								jQuery('.mainLoginButton').hide();
								// if (e)	e.preventdefault();
								return false;
							})
							
