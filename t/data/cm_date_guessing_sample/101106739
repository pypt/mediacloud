<script>

	jQuery(document).ready(function($) {

		//put content div class, when scroll beyond this y, float it
		var postShare = jQuery('.dd_ajax_float');

		if(jQuery('.dd_content_wrap').length > 0){

			var descripY = parseInt(jQuery('.dd_content_wrap').offset().top) + 0;
			var pullX = postShare.css('margin-left');
			var pullLeft  = postShare.css('left');
			var startingTop = postShare.css('top');

			jQuery(window).scroll(function () {

				var scrollY = jQuery(window).scrollTop();
				var fixedShare = postShare.css('position') == 'fixed';

				//make sure .post_share exists
				if(jQuery('.dd_ajax_float').length > 0){

					if ( scrollY > descripY && !fixedShare ) {
						postShare.stop().css({
							position: 'fixed',
							left: '50%',
							top: 40,
							marginLeft: -553

						});
					} else if ( scrollY < descripY && fixedShare ) {
						postShare.css({
							position: 'absolute',
							left: pullLeft,
							top: startingTop,
							marginLeft: pullX
						});
					}

				}

			});
		}
	});

	jQuery(document).ready(function($) {
		jQuery('.headerCont .header').css('zIndex',1000);
	});

</script>


