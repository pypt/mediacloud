	<script type="text/javascript">

		var lcr_options = {
		    chart: {
		        renderTo: 'lcr_chart_container',
		        defaultSeriesType: 'spline'
		    },
		    title: {
		        text: 'PROP35 Campaign Resources'
		    },
			xAxis: {
				type: 'datetime',
				dateTimeLabelFormats: { // don't display the dummy year
					week: '%b %e',
					year: '%b'
				}
			},
		    yAxis: {
		        title: {
		            text: 'Estimated Resources'
		        },
	            labels: {
	                formatter: function() {
	                    return '$' + (this.value/1000000) + 'M' ;
	                }
	            },
		        min: 0
		    },
			tooltip: {
				shared: true,
				crosshairs: true,
				valueDecimals: 0,
				valuePrefix: '$',
				pointFormat: '<span style="color:{series.color}">{series.name}</span>: <b>{point.y}</b><br/>'
			},
			plotOptions: {
				spline: {
					lineWidth: 2,
					states: {
						hover: {
							lineWidth: 2
						}
					},
					marker: {
						enabled: false,
						states: {
							hover: {
								enabled: true,
								symbol: 'circle',
								radius: 5,
								lineWidth: 1
							}
						}
					},
					pointInterval: 86400000, // one day
					pointStart: Date.UTC(2012, 9, 20, 0, 0, 0)
				}
			},
		    series: []
		};

		$.get('PROP35_lcr.csv', function(data) {
            // Split the lines
            var lines = data.split('\n');
            $.each(lines, function(lineNo, line) {
                var items = line.split(',');

				var series = {
				 data: []
				};
				
				$.each(items, function(itemNo, item)
				{
				 if (itemNo == 0) {
				    series.name = item.replace(/\"/g,'');
				 } else {
				    
				    series.data.push(parseFloat(item));
				 }
				
				});
				
				lcr_options.series.push(series);
		        
		    });
		    
		    // Remove empty csv line
		    lcr_options.series.pop();
		    
		    // Create the chart
		    var lcr_chart = new Highcharts.Chart(lcr_options);
		});


		var ie_options = {
		    chart: {
		        renderTo: 'ie_chart_container',
		        defaultSeriesType: 'spline'
		    },
		    title: {
		        text: 'PROP35 Independent Expenditures'
		    },
			xAxis: {
				type: 'datetime',
				dateTimeLabelFormats: { // don't display the dummy year
					week: '%b %e',
					year: '%b'
				}
			},
		    yAxis: {
		        title: {
		            text: 'Net IE Impact'
		        },
	            labels: {
	                formatter: function() {
	                    return '$' + (this.value/1000) + 'K' ;
	                }
	            }
		    },
			tooltip: {
				shared: true,
				crosshairs: true,
				valueDecimals: 0,
				valuePrefix: '$',
				pointFormat: '<span style="color:{series.color}">{series.name}</span>: <b>{point.y}</b><br/>'
			},
			plotOptions: {
				spline: {
					lineWidth: 2,
					states: {
						hover: {
							lineWidth: 2
						}
					},
					marker: {
						enabled: false,
						states: {
							hover: {
								enabled: true,
								symbol: 'circle',
								radius: 5,
								lineWidth: 1
							}
						}
					},
					pointInterval: 86400000, // one day
					pointStart: Date.UTC(2012, 8, 15, 0, 0, 0)
				}
			},
		    series: []
		};

		$.get('PROP35_ie.csv', function(data) {
            // Split the lines
            var lines = data.split('\n');
            $.each(lines, function(lineNo, line) {
                var items = line.split(',');

				var series = {
				 data: []
				};
				
				$.each(items, function(itemNo, item)
				{
				 if (itemNo == 0) {
				    series.name = item.replace(/\"/g,'');
				 } else {
				    
				    series.data.push(parseFloat(item));
				 }
				
				});
				
				ie_options.series.push(series);
		        
		    });
		    
		    // Remove empty csv line
		    ie_options.series.pop();
		    
		    // Create the chart
		    var ie_chart = new Highcharts.Chart(ie_options);
		});

	</script>
		


