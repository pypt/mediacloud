     if (($("#fbSponsored").attr("class") == "sponFb out") && ($("#fbSponsored").css("display") != "none") && ($("#fbSponsored").attr('class') == "sponFb out") && ($("#sponsored").attr('class') == "out") && ($("#defaultSponsored").attr("class") == "spon out") && (target3 ==0) && (target2 ==1)) {
