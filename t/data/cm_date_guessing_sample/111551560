nsu227661694.hasOmnitureFired = false;

nsu227661694.signUserUp = function(email) {
	var isAddressValid = nbc.validateEmail(email);
	var thisNewsletterID = "227661694";
	
		if(isAddressValid != true) {
		return false;
		} else {
		nsu227661694.checkUserStatus();
		}
		
	
	nsu227661694.fireOmniValues = function() {
