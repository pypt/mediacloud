         $(this).attr("class","spon out");
      });
   $("#defaultSponsored").mouseenter(function(event){
   $(this).attr("class","selected1");
   if (event.target == this) { U.log("here 0");}
   U.log(target1+" "+target2);
   if (($("#fbSponsored").css("display") == "none") && (target1==0) && (target2==0)) {
       target1=1;
       target3=0;
       target2=1;
