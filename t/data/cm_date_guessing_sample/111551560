                    weatherZipCode = "90012";
                  }
                  dataString="h=v3_topweathermodule&zipCode="+weatherZipCode;
                  var urlpost='http://www.nbclosangeles.com/i/dispatcher/';
                  jQuery.ajax({
                    type: "GET",
                    url: urlpost,
                    data: dataString,
                    cache:false,
                    dataType: "html",
                    success: function(data){
                      trimdata = jQuery.trim(data);
                                            $("#mastheadWeatherModule").html(trimdata);
                      $("#mastheadWeatherModule1").show();
                    }
                  });
                });
                //]]>
          </script>
